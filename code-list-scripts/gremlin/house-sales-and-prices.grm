

g.V().has('_code_list', 'listID', 'house-sales-and-prices').has('edition', 'one-off').
  fold().coalesce(unfold(), addV('_code_list').
    property(single, 'listID', "house-sales-and-prices").
    property(single, 'label', "House Sales And Prices").
    property(single, 'edition', "one-off")
  ).next()


g.V().has('_code','listID', 'house-sales-and-prices').has('value', "mean").
