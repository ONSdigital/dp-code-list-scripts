

g.V().has('_code_list', 'listID', 'measure-of-wellbeing').has('edition', 'one-off').
  fold().coalesce(unfold(), addV('_code_list').
    property(single, 'listID', "measure-of-wellbeing").
    property(single, 'label', "Measure Of Wellbeing").
    property(single, 'edition', "one-off")
  ).next()


g.V().has('_code','listID', 'measure-of-wellbeing').has('value', "worthwhile").
